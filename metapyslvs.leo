<t tx="leo.20231205084741.59">@Slot()
def __change_index(self) -&gt; None:
    """QTimer change index."""
    index = self.dial.value()
    speed = self.variable_speed.value()
    extreme_rebound = (
        self.conflict.isVisible()
        and self.extremeRebound.isChecked()
    )
    if extreme_rebound:
