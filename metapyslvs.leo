        appimage.add_argument(
            '--appimage-offset',
            action='store_true',
            help="obtain offset value of 'mount' command, then mount it with: "
                 "\"sudo mount PACKAGE MOUNT -o offset=VALUE\""
        )
    args = Arguments(**vars(parser.parse_args()))
    ARGUMENTS.__dict__.update(args.__dict__)


