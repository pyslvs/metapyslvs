<t tx="leo.20231205084738.160">def size_format(num: float) -&gt; str:
    """Calculate file size based on binary."""
    if num &lt; 0:
        raise ValueError("size must be positive value")
    s = "0 B"
    units = ('', 'K', 'M', 'G', 'T', 'P', 'E', 'Z', 'Y')
    for i, u in enumerate(units):
        s = f"{num:3.2f} {u}B"
        if abs(num) &lt; 1024:
            break
        num /= 1024
    return s
