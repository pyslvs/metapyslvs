        else:
            loop_list[anchor:anchor] = line
    elif len(line) &gt; s2 - (s1 + 1):
        anchor = s1 + 1
        del loop_list[anchor:s2]
        if loop_list[s1] == n2:
            # reverse
            loop_list[anchor:anchor] = reversed(line)
        else:
