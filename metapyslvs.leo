<t tx="leo.20231205084805.232">cpdef list link_assortment(Graph g):
    """Return link assortment of the graph."""
    assortment = [0]
    if not g.edges:
        return assortment
    @others
    for n in g.degrees().values():
        if n &lt; 2:
            continue
        d = n - 2
        while d &gt;= len(assortment):
            assortment.append(0)
        assortment[d] += 1
    return assortment


