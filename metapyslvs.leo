    """Normalization implementation."""
    @others
    for i in range(len(p1)):
        d = hypot(p1[i, 0], p1[i, 1])
        if d &lt; d_min:
            d_min = d
        if p1[i, 0] &gt; p1_max:
            p1_max = p1[i, 0]
            if p1[i, 1] &gt; 0:
                ind = i
        if p1[i, 0] &lt; p1_min:
            p1_min = p1[i, 0]
    d_min = p1_max - p1_min
    for i in range(len(p1)):
        p1[i, 0] /= d_min
        p1[i, 1] /= d_min
    # Swap to the starting point of the path
    roll(p1, ind)


