    """Test 'atlas' libraries."""
    answers = []
    # Test assortment [4]
    type_0 = [4]
    cg_list = contracted_graph(type_0)
    for c_j in contracted_link_synthesis(type_0):
        answer = conventional_graph(cg_list, c_j)
