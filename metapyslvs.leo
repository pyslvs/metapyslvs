    self.painter.drawLine(QLineF(neg_x, neg_y, pos_x, neg_y))
    self.painter.drawLine(QLineF(neg_x, pos_y, neg_x, neg_y))
    self.painter.drawLine(QLineF(pos_x, pos_y, pos_x, neg_y))

