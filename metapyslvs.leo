<t tx="leo.20231205084740.64">@Slot(name='on_rotate_btn_clicked')
def __rotate(self) -&gt; None:
    """Rotate by origin."""
    angle = radians(self.rotate_angle.value())
    path = []
    for x, y in self.path:
        h = hypot(x, y)
        a = atan2(y, x) + angle
        path.append((h * cos(a), h * sin(a)))
    self.set_path(path)
    self.accept()

