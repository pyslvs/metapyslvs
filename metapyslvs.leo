        if line[0] == line[-1]:
            inter = used_nodes &amp; g.adj[line[0]]
            start = inter.pop()
            end = inter.pop()
        else:
            inter = used_nodes &amp; g.adj[line[0]]
            start = inter.pop()
            inter = used_nodes &amp; g.adj[line[-1]]
            end = inter.pop()

