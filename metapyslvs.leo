<t tx="leo.20231205084806.32">def __richcmp__(self, Entity other, int op) -&gt; bint:
    """Compare the entities."""
    if op == Py_EQ:
        return (
            self.t == other.t and
            self.h == other.h and
            self.wp == other.wp and
            self.g == other.g and
            self.params == other.params
        )
    elif op == Py_NE:
        return not (self == other)
    else:
